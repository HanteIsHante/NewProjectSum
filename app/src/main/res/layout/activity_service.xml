<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_service"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.hante.newprojectsum.serviceactivity.ServiceActivity">
    <com.example.hante.newprojectsum.custome.TitleBar
        android:id="@+id/service_titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/share_weixin"
        android:paddingTop="@dimen/padding_top"
        android:fitsSystemWindows="true"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        />
</LinearLayout>
